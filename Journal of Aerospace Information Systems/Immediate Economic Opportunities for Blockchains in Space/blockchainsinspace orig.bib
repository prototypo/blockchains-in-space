	Doi = {10.1016/j.aei.2019.03.003},
	File = {ScienceDirect Full Text PDF:/Users/davidhyland-wood/Zotero/storage/PTWR76A8/Aremu et al. - 2019 - A Relative Entropy Weibull-SAX framework for healt.pdf:application/pdf;ScienceDirect Snapshot:/Users/davidhyland-wood/Zotero/storage/PD2FFIZU/S1474034618305603.html:text/html},
	Issn = {1474-0346},
	Journaltitle = {Advanced Engineering Informatics},
	Keywords = {Condition monitoring, Hidden Markov model, Information theory, Kalman filter, Machine learning, Predictive maintenance, Relative entropy, Weibull distribution},
	Pages = {121--134},
	Rights = {All rights reserved},
	Shortjournal = {Advanced Engineering Informatics},
	Title = {A Relative Entropy Weibull-{SAX} framework for health indices construction and health stage division in degradation modeling of multivariate time series asset data},
